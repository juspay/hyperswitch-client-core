let wifioff = `<svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-wifi-off" viewBox="0 0 16 16"><path d="M10.706 3.294A12.6 12.6 0 0 0 8 3C5.259 3 2.723 3.882.663 5.379a.485.485 0 0 0-.048.736.52.52 0 0 0 .668.05A11.45 11.45 0 0 1 8 4q.946 0 1.852.148zM8 6c-1.905 0-3.68.56-5.166 1.526a.48.48 0 0 0-.063.745.525.525 0 0 0 .652.065 8.45 8.45 0 0 1 3.51-1.27zm2.596 1.404.785-.785q.947.362 1.785.907a.482.482 0 0 1 .063.745.525.525 0 0 1-.652.065 8.5 8.5 0 0 0-1.98-.932zM8 10l.933-.933a6.5 6.5 0 0 1 2.013.637c.285.145.326.524.1.75l-.015.015a.53.53 0 0 1-.611.09A5.5 5.5 0 0 0 8 10m4.905-4.905.747-.747q.886.451 1.685 1.03a.485.485 0 0 1 .047.737.52.52 0 0 1-.668.05 11.5 11.5 0 0 0-1.811-1.07M9.02 11.78c.238.14.236.464.04.66l-.707.706a.5.5 0 0 1-.707 0l-.707-.707c-.195-.195-.197-.518.04-.66A2 2 0 0 1 8 11.5c.374 0 .723.102 1.021.28zm4.355-9.905a.53.53 0 0 1 .75.75l-10.75 10.75a.53.53 0 0 1-.75-.75z"/></svg>`

type uri = {
  uri: string,
  local: bool,
}

open ReactNative
open Style

@react.component
let make = React.memo((
  ~name,
  ~width=20.,
  ~height=16.,
  ~fill="#ffffff",
  ~style=empty,
  ~fallbackIcon: option<string>=?,
) => {
  let (isLoaded, setIsLoaded) = React.useState(_ => false)
  let (iconName, setIconName) = React.useState(_ =>
    name->String.replaceRegExp(%re("/ /g"), "")->String.toLowerCase
  )

  React.useEffect1(() => {
    setIconName(_ => name->String.replaceRegExp(%re("/ /g"), "")->String.toLowerCase)
    None
  }, [name])

  let getAssetUrl = GlobalHooks.useGetAssetUrlWithVersion()

  let uri = React.useMemo1(() => {
    let assetUrl = `${getAssetUrl()}/images/error.svg`

    let localName = switch iconName {
    | "error" => ReactNativeSvg.error
    | "card" => card
    | "cardv1" => cardv1
    | "close" => close
    | "cvv" => cvv
    | "lock" => lock
    | "waitcard" => waitcard
    | "camera" => camera
    | "addwithcircle" => addwithcircle
    | "checkboxclicked" => checkboxclicked
    | "checkboxnotclicked" => checkboxnotclicked
    | "defaulttick" => defaultTick
    | "googlepay" => google_pay
    | "applepay" => applePayList
    | "samsung_pay" => samsungPay
    | "becsdebit"
    | "bacsdebit" => becsDebit
    | "cartesbancaires" => cartesBancaires
    | "sepadebit" => sepadebit
    | "achdebit"
    | "achbanktransfer" => achbankdebit
    | "bank" => bankicon
    | "disclaimer" => disclaimericon
    | "add" => add
    | "mappin" => mapPin
    | "paysafecard" => paysafecard
    | "paysafecard2" => paysafecard2
    | "skrill" => skrill
    | "skrill2" => skrill2
    | "saved" => saved
    | "visa" => visa
    | "mastercard" => mastercard
    | "americanexpress" => americanexpress
    | "dinersclub" => dinersclub
    | "discover" => discover
    | "jcb" => jcb
    | "chevron" => chevron
    | "klarna2" => klarnaFull
    | "wifi" => wifi
    | "wifioff" => wifioff
    | _ => ""
    }
    localName == ""
      ? {
          uri: assetUrl->String.replace("error", iconName),
          local: false,
        }
      : {uri: localName, local: true}
  }, [iconName])

  <View style={array([s({height: height->dp, width: width->dp}), style])}>
    {uri.local
      ? <ReactNativeSvg.SvgCss
          onError={() => {
            setIsLoaded(_ => true)
            setIconName(_ => fallbackIcon->Option.getOr("error"))
          }}
          onLoad={() => {
            setIsLoaded(_ => true)
          }}
          xml=uri.uri
          width
          height
          fill
        />
      : <ReactNativeSvg.SvgUri
          onError={() => {
            setIsLoaded(_ => true)
            setIconName(_ => fallbackIcon->Option.getOr("error"))
          }}
          onLoad={() => {
            setIsLoaded(_ => true)
          }}
          uri=uri.uri
          width
          height
          fill
        />}
    {isLoaded || uri.local
      ? React.null
      : <ActivityIndicator style={s({height: height->dp, width: width->dp})} color=fill />}
  </View>
})
